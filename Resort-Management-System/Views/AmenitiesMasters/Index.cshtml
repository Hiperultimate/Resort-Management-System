@model IEnumerable<Resort_Management_System.Models.AmenitiesMaster>

@{
    ViewBag.Title = "Index";
}
@Styles.Render("~/Styles/StyleSheetCommon.css")
@Styles.Render("~/Styles/StyleSheetIndex.css")

<div class="displayTop">
    <h2 class="allDetail"><span>ALL AMENITIES</span></h2>
    <input type="button" class="button2" value="Add New Amenity" onclick="location.href='@Url.Action("Create", "AmenitiesMasters")'" />
</div>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AmenityName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AmenityName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.AmenityID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.AmenityID })
        </td>
    </tr>
}

</table>

