@model IEnumerable<Resort_Management_System.Models.AmenitiesMaster>

@{
    ViewBag.Title = "Index";
}

<h2>Amenity</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AmenityName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AmenityName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.AmenityID }) |
            @Html.ActionLink("Details", "Details", new { id=item.AmenityID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.AmenityID })
        </td>
    </tr>
}

</table>

<h4>@Html.ActionLink("Add A New Amenity", "Create")</h4>